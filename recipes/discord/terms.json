{
  "url": "https://discord.com/terms",
  "selector": "div.section-rich",
  "enabled": true,
  "rules": [
    {
      "name": "remove-nav",
      "filter": "nav",
      "replacement": ""
    },
    {
      "name": "remove-header",
      "filter": "header",
      "replacement": ""
    },
    {
      "name": "remove-footer",
      "filter": "footer",
      "replacement": ""
    },
    {
      "name": "remove-banner",
      "filter": "[class*='banner']",
      "replacement": ""
    }
  ]
}
