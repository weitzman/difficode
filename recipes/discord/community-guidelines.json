{
  "url": "https://discord.com/guidelines",
  "selector": ".section-rich",
  "enabled": true,
  "rules": [
    {
      "name": "remove-nav",
      "filter": "nav",
      "replacement": ""
    },
    {
      "name": "remove-header",
      "filter": "header",
      "replacement": ""
    },
    {
      "name": "remove-footer",
      "filter": "footer",
      "replacement": ""
    },
    {
      "name": "remove-banner",
      "filter": "[class*='banner']",
      "replacement": ""
    },
    {
      "name": "remove-toc",
      "filter": "a.link-terms",
      "replacement": ""
    },
    {
      "name": "remove-archive-link",
      "filter": "a.archived-link",
      "replacement": ""
    }
  ]
}
